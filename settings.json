{
    "explorer.confirmDelete": false,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "d.dubCompiler": "dmd.EXE",
    "d.stdlibPath": "auto",
    "d.dubPath": "C:\\D\\dmd2\\windows\\bin\\dub.exe",
    "d.servedPath": "C:\\Users\\olive\\AppData\\Roaming\\code-d\\bin\\serve-d.exe",
    "files.autoSave": "afterDelay",
